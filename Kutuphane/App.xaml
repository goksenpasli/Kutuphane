            <DataTemplate x:Key="ListViewDolapTemplate">
                <StackPanel Orientation="Horizontal">
                    <Image Width="32" Height="32">
                        <Image.Source>
                            <MultiBinding Converter="{StaticResource SystemFileIconConverter}">
                                <Binding Source="Moricons.dll" />
                                <Binding Source="105" />
                            </MultiBinding>
                        </Image.Source>
                    </Image>
                    <TextBlock VerticalAlignment="Center">
                        <Run Text="{Binding Açıklama}" Foreground="Blue" FontStyle="Italic" />
                        <Run Text="{Binding Kod, StringFormat=\{0:000\}}" Foreground="Red" FontWeight="Bold" />
                    </TextBlock>
                </StackPanel>
            </DataTemplate>
            <Style TargetType="ListView">
                <Setter Property="VirtualizingPanel.CacheLengthUnit" Value="Item" />
                <Setter Property="VirtualizingPanel.ScrollUnit" Value="Pixel" />
                <Setter Property="Extensions:GridViewSort.AutoSort" Value="True" />
                <Setter Property="ScrollViewer.IsDeferredScrollingEnabled" Value="True" />
            </Style>
            <Style TargetType="Extensions:ContentToggleButton">
                <Setter Property="CornerRadius" Value="0" />
            </Style>
            <Kutuphane:DataImageFilePathImageUriConverter x:Key="LocalDataImageFilePathImageUriConverter" Kutuphane:DataImageFilePathImageUriConverter.DecodeHeight="120" />
            <DataTemplate x:Key="KişiTemplate">
                <StackPanel Orientation="Horizontal">
                    <ViewModel:ShadowedImage MaxWidth="72" MaxHeight="72"
                        ShowOverlayColor="{Binding KitapAlabilir, Converter={StaticResource ReverseBooleanConverter}, Mode=OneWay}"
                        ShowShadow="True">
                        <Image.Style>
                            <Style TargetType="Image">
                                <Style.Triggers>
                                    <DataTrigger Binding="{Binding KişiResimGöster, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged, Source={x:Static properties:Settings.Default}}" Value="true">
                                        <Setter Property="Source">
                                            <Setter.Value>
                                                <Binding Converter="{StaticResource LocalDataImageFilePathImageUriConverter}" Path="Resim" />
                                            </Setter.Value>
                                        </Setter>
                                    </DataTrigger>
                                </Style.Triggers>
                            </Style>
                        </Image.Style>
                    </ViewModel:ShadowedImage>
                    <TextBlock Text="{Binding Ad}" VerticalAlignment="Center" />
                </StackPanel>
            </DataTemplate>
            <DataTemplate x:Key="KişiDetayTemplate">
                <StackPanel Orientation="Horizontal">
                    <ViewModel:ShadowedImage MaxWidth="120" MaxHeight="120"
                        ShowOverlayColor="{Binding KitapAlabilir, Converter={StaticResource ReverseBooleanConverter}, Mode=OneWay}"
                        ShowShadow="True">
                        <Image.Style>
                            <Style TargetType="Image">
                                <Style.Triggers>
                                    <DataTrigger Binding="{Binding KişiResimGöster, Mode=OneWay, Source={x:Static properties:Settings.Default}}" Value="true">
                                        <Setter Property="Source">
                                            <Setter.Value>
                                                <Binding Converter="{StaticResource LocalDataImageFilePathImageUriConverter}" Path="Resim" />
                                            </Setter.Value>
                                        </Setter>
                                    </DataTrigger>
                                </Style.Triggers>
                            </Style>
                        </Image.Style>
                    </ViewModel:ShadowedImage>
                    <GroupBox>
                        <StackPanel>
                            <TextBlock FontSize="24" FontWeight="Bold">
                                <Run Text="{Binding Ad}" />
                                <Run Text="{Binding Soyad}" />
                            </TextBlock>
                            <TextBlock Text="{Binding TC}" FontSize="24" FontStyle="Italic" />
                            <Extensions:MaskedTextBlock FontSize="18" InputMask="(000) 000 00 00" UnmaskedText="{Binding Telefon}" />
                        </StackPanel>
                    </GroupBox>
                </StackPanel>
            </DataTemplate>
            <DataTemplate x:Key="KitapTemplate">
                <StackPanel Background="{Binding Renk, Converter={StaticResource StringToBrushConverter}}">
                    <TextBlock Text="{Binding Ad}" />
                    <ViewModel:ShadowedImage MaxWidth="110" MaxHeight="145"
                        ShowOverlayColor="{Binding ÖdünçVerilebilir, Converter={StaticResource ReverseBooleanConverter}, Mode=OneWay}"
                        ShowShadow="True">
                        <Image.Style>
                            <Style TargetType="Image">
                                <Style.Triggers>
                                    <DataTrigger Binding="{Binding KitapResimGöster, Mode=OneWay, Source={x:Static properties:Settings.Default}}" Value="true">
                                        <Setter Property="Source">
                                            <Setter.Value>
                                                <Binding Converter="{StaticResource LocalDataImageFilePathImageUriConverter}" Path="Resim" />
                                            </Setter.Value>
                                        </Setter>
                                    </DataTrigger>
                                </Style.Triggers>
                            </Style>
                        </Image.Style>
                    </ViewModel:ShadowedImage>
                </StackPanel>
            </DataTemplate>
            <ControlTemplate x:Key="ComboBoxSimpleTemplate" TargetType="{x:Type ComboBox}">
                <ContentPresenter Content="{TemplateBinding SelectionBoxItem}" ContentTemplate="{TemplateBinding SelectionBoxItemTemplate}" IsHitTestVisible="False" />
            </ControlTemplate>
            <DataTemplate x:Key="KitapComboBoxTemplate">
                <ComboBox IsEnabled="False"
                    ItemsSource="{Binding Kütüphane.Kitaplar, Source={StaticResource MainViewModel}}"
                    ItemTemplate="{StaticResource KitapTemplate}"
                    SelectedValue="{Binding KitapId}"
                    SelectedValuePath="Id"
                    Template="{StaticResource ComboBoxSimpleTemplate}" />
            </DataTemplate>
            <ItemsPanelTemplate x:Key="ColorPanelTemplate">
                <UniformGrid Columns="8" />
            </ItemsPanelTemplate>
            <DataTemplate x:Key="ColorItemTemplate">
                <StackPanel Orientation="Horizontal">
                    <Rectangle Width="16" Height="16" Fill="{Binding Name}" />
                    <TextBlock Text="{Binding Name}" VerticalAlignment="Center" />
                </StackPanel>
            </DataTemplate>
            <Image x:Key="DemirbaşImage"
                Width="16" Height="16"
                x:Shared="false">
                <Image.Style>
                    <Style TargetType="{x:Type Image}">
                        <Style.Triggers>
                            <DataTrigger Binding="{Binding Demirbaş}" Value="true">
                                <Setter Property="Source">
                                    <Setter.Value>
                                        <MultiBinding Converter="{StaticResource SystemFileIconConverter}">
                                            <Binding Source="Moricons.dll" />
                                            <Binding Source="65" />
                                        </MultiBinding>
                                    </Setter.Value>
                                </Setter>
                            </DataTrigger>
                        </Style.Triggers>
                    </Style>
                </Image.Style>
            </Image>
        </ResourceDictionary>
    </Application.Resources>
</Application>
